<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.SystemUI.Dialog" parent="@android:style/Theme.Material.Dialog">
        <item name="android:colorControlActivated">#ffeeeeee</item>
        <item name="android:colorPrimary">@android:color/bright_foreground_dark</item>
    </style>
    <style name="qs_base" parent="@android:style/Theme.DeviceDefault">
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="android:colorControlNormal">@android:color/white</item>
        <item name="android:colorPrimary">@android:color/link_text_holo_light</item>
        <item name="android:colorPrimaryDark">@android:color/link_text_holo_light</item>
    </style>
    <style name="qs_theme">
        <item name="android:windowIsFloating">true</item>
        <item name="android:panelColorBackground">@android:color/background_holo_dark</item>
        <item name="android:dividerHorizontal">@null</item>
        <item name="android:colorPrimary">@android:color/background_holo_dark</item>
    </style>
</resources>
